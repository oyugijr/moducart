# API Gateway example environment file
# Copy to .env and edit values for your environment

# Session secret for express-session
SESSION_SECRET=change_this_to_a_secure_value

# Backend service URLs (use Docker service names if running in compose)
CATALOG_API_URL=http://localhost:4001
BASKET_API_URL=http://localhost:4002
DISCOUNT_API_URL=http://localhost:4003
ORDERING_API_URL=http://localhost:4004

# Server port
PORT=5000
